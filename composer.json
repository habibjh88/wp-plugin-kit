{
	"name": "therakib7/wp-plugin-kit",
	"description": "A starter plugin for WordPress that provides a foundational structure to build upon.",
	"version": "0.1.0",
	"license": "GPL-3-or-later",
	"authors": [
		{
			"name": "Rakib",
			"email": "therakib7@gmail.com"
		}
	],
	"require": {
		"php": "^7.4 || ^8.0"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^3.0",
		"tareq1988/wp-php-cs-fixer": "dev-master",
		"phpcompatibility/phpcompatibility-wp": "*"
	},
	"autoload": {
		"psr-4": {
			"WpPluginKit\\": "includes"
		}
	},
	"scripts": {
		"phpcs": [
			"phpcs -ps"
		],
		"phpcbf": [
			"phpcbf -p"
		]
	},
	"minimum-stability": "dev",
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}